                        <section class="bar_header">
                            <img src="images/logo-one-column.png" class="logo"/>
                            <div class="dropdown profileDropdown">
                                <button class="btn btn-default dropdown-toggle profileButton" type="button" data-toggle="dropdown"><i class="fi-torso" id="profileIcon"></i> | Profile
