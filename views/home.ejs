<script>
    (function () {

        console.log("FUNTION :");
        var container = document.getElementById('appContainer-fixed--highlight');
        [].slice.call(document.querySelectorAll('.appBank')).forEach(function (el) {
            var openCtrl = el.querySelector('.appBank__button'),
                closeCtrl = el.querySelector('.appBank__cancel');

            openCtrl.addEventListener('click', function (ev) {
                ev.preventDefault();
                classie.add(el, 'appBank--active');
                classie.add(container, 'appContainer-fixed--highlight');
            });

            closeCtrl.addEventListener('click', function () {
                classie.remove(el, 'appBank--active');
                classie.remove(container, 'appContainer-fixed--highlight');
            });
        });
    })();
