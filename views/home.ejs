                            </div>
                        </section>
                    </div>

                    <!-------------------------page 2--------------------->
                    <div class="page" id="p2">

                        <section class="bar_header">
                            <img src="images/logo-one-column.png" class="logo"/>
                            <div class="dropdown profileDropdown">
                                <button class="btn btn-default dropdown-toggle profileButton" type="button" data-toggle="dropdown"><i class="fi-torso" id="profileIcon"></i> | Profile
                                    <span class="caret"></span></button>
                                <ul class="dropdown-menu">
                                    <li><a href="newApplicant">New Applicant</a></li>
                                    <li><a href="myProfile">View</a></li>
                                    <li><a href="editProfile">Edit</a></li>
                                    <li class="divider"></li>
                                    <li><a href="logout">Logout</a></li>
                                </ul>
                            </div>
                        </section>

                        <section class="icon" id="overview_icon"><i class="fi-results"></i><span class="title">Overview</span>
                            <p class="hint">Discover what our internship program is all about!</p>
                            <section class="icon" id="down_arrow"><a href="#overview_content" id="arrow_nav"><i class="fi-arrow-down"></i></a></section>
                        </section>

                        <section class="content" id="overview_content">
                            <div class="sect_container">
                                <div class="media-object stack-for-small" id="overview_media">
                                    <div class="media-object-section">
                                        <div class="thumbnail">
                                            <img src="images/Interns2016.png" class="medium">
                                        </div>

                                    </div>
                                    <div class="media-object-section">
                                        <div class="text">
                                            <h4 id="text_title"><%= mediaObjectsOverviewData[0].mediaTitle %></h4>
                                            <hr>
                                            <p><%= mediaObjectsOverviewData[0].mediaText %></p>
                                        </div>
                                    </div>
                                </div>
                            </div>


                            <div class="section_container">
                                <h4>Current Positions</h4>
                                <table class="stack" id="myTable">
                                    <thead class="job_header">
                                    <tr>
                                        <th width="250" height="auto" id="job_bar"><p><strong>Job Title</strong></p></th>
                                        <th><p><strong>Description</strong></p></th>
                                    </tr>
                                    </thead>
                                    <tfoot class="job_header">
                                    <tr>
                                        <th width="250" height="auto"></th>
                                        <th><p><a href="#t3" class="button" id="button_pos" onclick="open_tab('example-vert-tabs','panel1v')">View / Apply</a></p></th>
                                    </tr>
                                    </tfoot>

                                    <tbody>
                                    <% for(var i = 0; i < jobBoardData.length; i++) { %>
                                    <tr id="row<%= i + 1 %>" onclick="open_tab('example-vert-tabs','panel<%= i + 1 %>v')">
                                        <td><a id="item<%= i + 1 %>" href=#t3>
                                                <div>
                                                    <p><strong><%= jobBoardData[i].projTitle %></strong></p>
                                                </div>
                                            </a></td>
                                        <td><a id="item<%= i + 1 %>" href=#t3>
                                                <div>
                                                    <em><%= jobBoardData[i].projDescription %></em>
                                                </div>
                                            </a></td>
                                    </tr>
                                    <% } %>
                                    </tbody>
                                </table>
                            </div>

                            <div class="sect_container">
                                <div class="table_title"><h4>Past Interns</h4></div>

                                <div class="intern_table">

                                    <ul class="tabs" data-tabs id="example-tabs">
                                        <% for(var i = 0; i < mediaObjectsPastInternData.length; i++) { %>
                                        <% if(i === 0) { %>
                                        <li class="tabs-title is-active"><a href="#panel<%= i + 1 %>" aria-selected="true"
                                                                            id="tabs_title tab<%= i + 1 %>"><%= mediaObjectsPastInternData[i].mediaTitle %></a></li>
                                        <% } else { %>
                                        <li class="tabs-title"><a href="#panel<%= i + 1 %>" id="tabs_title tab<%= i + 1 %>"><%= mediaObjectsPastInternData[i].mediaTitle %></a></li>
                                        <% }} %>
                                    </ul>

                                    <div class="tabs-content" data-tabs-content="example-tabs">
                                        <% for(var i = 0; i < mediaObjectsPastInternData.length; i++) { %>
                                        <% if(i === 0) { %>
                                        <div class="tabs-panel is-active" id="panel<%= i + 1 %>">
                                            <p><%= mediaObjectsPastInternData[i].mediaText %></p>
                                        </div>
                                        <% } else  { %>
                                        <div class="tabs-panel" id="panel<%= i + 1 %>">
                                            <p><%= mediaObjectsPastInternData[i].mediaText %></p>
                                        </div>
                                        <% } %>
                                        <% } %>
                                    </div>

                                </div>
                            </div>
                        </section>
                    </div>

                    <!page 3--------------------->

                    <div class="page" id="p3">
                        <section class="bar_header">
                            <img src="images/logo-one-column.png" class="logo"/>
                            <div class="dropdown profileDropdown">
                                <button class="btn btn-default dropdown-toggle profileButton" type="button" data-toggle="dropdown"><i class="fi-torso" id="profileIcon"></i> | Profile
                                    <span class="caret"></span></button>
                                <ul class="dropdown-menu">
                                    <li><a href="newApplicant">New Applicant</a></li>
                                    <li><a href="myProfile">View</a></li>
                                    <li><a href="editProfile">Edit</a></li>
                                    <li class="divider"></li>
                                    <li><a href="logout">Logout</a></li>
                                </ul>
                            </div>
                        </section>
                        <section class="icon" id="job_board_icon"><i class="fi-checkbox"></i><span class="title">Job Board</span>
                            <p class="hint">Apply for our current positions here! If you haven't already, please create a <a href="newApplicant">new applicant profile</a>!</p>
                            <section class="icon" id="down_arrow"><a href="#app_container" id="arrow_nav" target="_top"><i class="fi-arrow-down"></i></a></section>
                        </section>

                        <! ---- iterate for the tab views ----->
                        <section class="content" id="job_board_content">

                            <div class="row expanded" id="app_container">

                                <ul class="tabs vert_tabs" id="example-vert-tabs" data-tabs>

                                    <% for(var i = 0; i < jobBoardData.length; i++) { %>

                                    <! AJE add check to see if they applied --------------------->
                                    <% var colorString = "Black"; %>
                                    <% for(var z = 0; z < appFolderData.length; z++) { %>
                                    <% if(typeof appFolderData[z].idProject_fk !== 'undefined' && (appFolderData[z].idProject_fk === (jobBoardData[i].idProject))) { %>
                                    <% colorString = "Blue"; %>
                                    <% } %>
                                    <% } %>

                                    <% if(i === 0) { %>
                                    <li class="tabs-title is-active"><a href="#panel<%= i + 1 %>v"><h6><font color="<%= colorString %>"><%= jobBoardData[i].projTitle %></font></h6></a></li>
                                    <% } else { %>
                                    <li class="tabs-title"><a href="#panel<%= i + 1 %>v"><h6><font color="<%= colorString %>"><%= jobBoardData[i].projTitle %> </font></h6></a></li>
                                    <% }} %>

                                </ul>

                                <div class="tabs-content" data-tabs-content="example-vert-tabs" id="vert_con">

                                    <% for(var i = 0; i < jobBoardData.length; i++) { %>
                                    <div class="tabs-panel <% if(i === 0 ) { %>is-active<% } %>" id="panel<%= i + 1 %>v">

                                        <div class="title-bar" id="title_bar">
                                            <form action='myFolder' method="POST">
                                                <input type="text" value="<%= jobBoardData[i].idProject %>" name="idProject_fk" style="display:none;"/>
                                                <div class="title-bar-left"><h5><strong><%= jobBoardData[i].projTitle %> / <%= jobBoardData[i].projManager %></strong></h5></div>
                                                <div class="title-bar-right">
                                                    <button type="submit" onclick="applyApplicant()">

                                                        <! AJE add check to see if they applied --------------------->
                                                        <% var applyString = "Apply"; %>
                                                        <% for(var z = 0; z < appFolderData.length; z++) { %>
                                                        <% if(appFolderData[z].idProject_fk !== 'undefined' && (appFolderData[z].idProject_fk === (jobBoardData[i].idProject))) { %>
                                                        <% applyString = "Applied"; %>
                                                        <% } %>
                                                        <% } %>

                                                        <a class="success button"
                                                           style="width:300%;padding:1rem;margin-left:-200%;color:#4e4e4f;margin-bottom:0rem;"><strong><%= applyString %></strong></a></button>
                                                </div>
                                            </form>
                                        </div>

                                        <div class="detail_container">
                                            <div class="row">
                                                <div class="large-12 medium-12 columns">
                                                    <div class="title-bar" id="skill_bar">
                                                        <div class="title-bar-left"><h6>Project Detail</h6></div>
                                                        <div class="title-bar-right"></div>
                                                    </div>
                                                    <div id="text">
                                                        <h6><strong>Description</strong></h6>

                                                        <%= jobBoardData[i].projDescription %>

                                                        <ol>
                                                            <% projBrief = jobBoardData[i].projBrief; %>
                                                            <% listItems = projBrief.split(' | '); %>
                                                            <% for(var j = 0; j < listItems.length; j++) { %>
                                                            <li><%= listItems[j] %></li>
                                                            <% } %>

                                                        </ol>
                                                        <hr>
                                                        <h6><strong>Duties</strong></h6>

                                                        <%= jobBoardData[i].duties %>
                                                        <hr>

                                                        <h6><strong>Tasks</strong></h6>
                                                        <ul>
                                                            <% tasks = jobBoardData[i].tasks; %>
                                                            <% taskItems = tasks.split(' | '); %>
                                                            <% for(var j = 0; j < taskItems.length; j++) { %>
                                                            <li><%= taskItems[j] %></li>
                                                            <% } %>
                                                        </ul>
                                                        <hr>
                                                        <h6><strong>Education &amp; Experience</strong></h6>

                                                        <%= jobBoardData[i].eduExp %>
                                                        <hr>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <% } %>

                                </div>
                            </div>
                        </section>
                    </div>

                    <! page 4--------------------->
                    <div class="page" id="p4">
                        <section class="bar_header">
                            <img src="images/logo-one-column.png" class="logo"/>
                            <div class="dropdown profileDropdown">
                                <button class="btn btn-default dropdown-toggle profileButton" type="button" data-toggle="dropdown"><i class="fi-torso" id="profileIcon"></i> | Profile
                                    <span class="caret"></span></button>
                                <ul class="dropdown-menu">
                                    <li><a href="newApplicant">New Applicant</a></li>
                                    <li><a href="myProfile">View</a></li>
                                    <li><a href="editProfile">Edit</a></li>
                                    <li class="divider"></li>
                                    <li><a href="logout">Logout</a></li>
                                </ul>
                            </div>
                        </section>
                        <section class="icon" id="calendar_icon">
                            <i class="fi-calendar"></i>
                            <span class="title">Events</span>
                            <p class="hint">Find out information on Career Fairs and interviews!</p>
                            <section class="icon" id="down_arrow"><a href="#events_content" id="arrow_nav"><i class="fi-arrow-down" id="arrow_nav"></i></a></section>
                        </section>

                        <section class="content" id="events_content">
                            <div class="row">
                                <div class="large-11 large-centered columns">
                                    <h3>Upcoming Events:</h3>
                                </div>
                            </div>
                            <table class="stack">
                                <thead>
                                <tr>
                                    <th>Name</th>
                                    <th>Location</th>
                                    <th>Date</th>
                                    <th>Time</th>
                                </tr>
                                </thead>
                                <tbody>
                                <% for (var i = 0; i < eventsData.length; i++ ) { %>
                                <tr>
                                    <td><%= eventsData[i].eventTitle %></td>
                                    <td><%= eventsData[i].eventLocation %></td>
                                    <td><%= eventsData[i].eventDate %></td>
                                    <td><%= eventsData[i].startTime %> - <%= eventsData[i].endTime %></td>
                                </tr>
                                <% } %>
                                </tbody>
                            </table>

                        </section>
                    </div>

                    <!-------------------------page 5--------------------->
                    <div class="page" id="p5">
                        <section class="bar_header">
                            <img src="images/logo-one-column.png" class="logo"/>
                            <div class="dropdown profileDropdown">
                                <button class="btn btn-default dropdown-toggle profileButton" type="button" data-toggle="dropdown"><i class="fi-torso" id="profileIcon"></i> | Profile
                                    <span class="caret"></span></button>
                                <ul class="dropdown-menu">
                                    <li><a href="newApplicant">New Applicant</a></li>
                                    <li><a href="myProfile">View</a></li>
                                    <li><a href="editProfile">Edit</a></li>
                                    <li class="divider"></li>
                                    <li><a href="logout">Logout</a></li>
                                </ul>
                            </div>
                        </section>
                        <section class="icon" id="contact_icon">
                            <i class="fi-address-book"></i>
                            <span class="title">Contact</span>
                            <p class="hint">Reach out to members of our Human Resources department!</p>
                            <section class="icon" id="down_arrow"><a href="#contact_content" id="arrow_nav"><i class="fi-arrow-down" id="arrow_nav"></i></a></section>
                        </section>

                        <section class="content" id="contact_content">
                            <div style="">
                                <div class="row">
                                    <div class="large-4 columns large-centered">
                                        <img class="img-circle" src="images/user-default.png"/>
                                    </div>
                                </div>
                                <div class="row" id="contactName">
                                    <div class="large-6 columns large-centered">
                                        <h2>Amanda Ressel</h2>
                                    </div>
                                </div>
                                <div class="row" id="contactTitle">
                                    <div class="large-6 columns large-centered">
                                        <h3><em>Program Administrator</em></h3>
                                    </div>
                                </div>
                                <div class="row" id="contactPhone">
                                    <div class="large-6 columns large-centered">
                                        <h4>(217) 353 - 7520</h4>
                                    </div>
                                </div>
                                <div class="row" id="contactEmail">
                                    <div class="large-6 columns large-centered">
                                        <h4><a>internships@amdocs.com</a></h4>
                                    </div>
                                </div>
                            </div>
                        </section>
                    </div>

                    <! CURRENT APPLICATION BANK CODE ------------------------------------------------>

                    <div class="appContainer-fixed">
                        <i class="fi-folder folder__icon" width="30px" height="30px" viewBox="0 0 35 35"></i>
                        <h3> | My Applications</h3>
                        <div class="appBank__count"><%= appFolderData.length %></div>
                        <div class="appBank">
                            <a class="appBank__button" id="appBankButton"><!-- Fallback location -->
                                <span class="appBank__text">
							        <span class="appBank__text-inner appBank__initial-text">View Folder</span>
							        <span class="appBank__text-inner appBank__final-text"><div class="skillsFormButton"><i class=" fi-checkbox"></i> Submit </div></span>
						        </span>
                            </a>

                            <div class="appBank__results">
                                <div class="appBank__results-inner">
                                    <div class="appBank__summary">
                                        <div class="expanded row">
                                            <div class="large-12 columns">
                                                <div class="panel panel-primary currentAppPanel">
                                                    <div class="panel-heading">
                                                        <h5>Current Applications</h5>
                                                    </div>
                                                    <div class="panel-body">
                                                        <div class="row">
                                                            <div class="large-5 small-12 columns">
                                                                <div class="panel panel-info userDetailPanel">
                                                                    <div class="panel panel-heading">
                                                                        <h6>Personal Information</h6>
                                                                    </div>
                                                                    <div class="panel panel-body">

                                                                        <% if (!userData) { %>
                                                                        <div class="row">
                                                                            <div class="large-10 columns large-centered">
                                                                                <h5 style="color:dimgray;">If this is your first time visiting, please create a <a href="newApplicant">New Applicant
                                                                                        Profile</a>!</h5>
                                                                            </div>
                                                                        </div>
                                                                        <p style="color:dimgray;text-align:left;">
                                                                            <strong>Name</strong>: <br>
                                                                            <strong>Resume</strong>: <br>
                                                                            <strong>Email</strong>: <br>
                                                                            <strong>Address</strong>: <br>
                                                                            <strong>City</strong>: <br>
                                                                            <strong>State</strong>: <br>
                                                                            <strong>ZIP</strong>: <br>
                                                                            <strong>Phone</strong>: <br>
                                                                            <strong>Veteran Status</strong>: <br>
                                                                            <strong>US Work Authorization</strong>: <br>
                                                                            <strong>Industry Preference</strong>: <br>
                                                                            <strong>Foreign Language Skills</strong>: <br>
                                                                            <strong>School/University</strong>: <br>
                                                                            <strong>Degree</strong>: <br>
                                                                            <strong>Major</strong>: <br>
                                                                            <strong>Interests</strong>: <br>
                                                                            <strong>Expected Graduation</strong>:
                                                                        </p>
                                                                        <% } else { %>
                                                                        <% console.log("AJE : applicant view : " + userData.email); %>
                                                                        <p style="color:dimgray;text-align:left;">
                                                                            <strong>Name</strong>: <%= userData.nameFirst %> <%= userData.nameLast %><br>
                                                                            <strong>Resume</strong>: <a href="uploads/?fileName=<%= userData.resLink %>"><%= userData.resLink %></a><br>
                                                                            <strong>Email</strong>: <%= userData.email %><br>
                                                                            <strong>Address</strong>: <%= userData.address %><br>
                                                                            <strong>City</strong>: <%= userData.city %><br>
                                                                            <strong>State</strong>: <%= userData.stateCode %><br>
                                                                            <strong>ZIP</strong>: <%= userData.zip %><br>
                                                                            <strong>Phone</strong>: <%= userData.phone %><br>
                                                                            <strong>Veteran Status</strong>: <%= userData.usVet %><br>
                                                                            <strong>US Work Authorization</strong>: <%= userData.usAuth %><br>
                                                                            <strong>Industry Preference</strong>: <%= userData.industryPref %><br>
                                                                            <strong>Foreign Language Skills</strong>: <%= userData.langSkills %><br>
                                                                            <strong>School/University</strong>: <%= userData.school %><br>
                                                                            <strong>Degree</strong>: <%= userData.degree %><br>
                                                                            <strong>Major</strong>: <%= userData.major %><br>
                                                                            <strong>Interests</strong>: <%= userData.interest %><br>
                                                                            <strong>Expected Graduation</strong>: <%= userData.gradDate %>
                                                                        </p>
                                                                        <% } %>

                                                                    </div>
                                                                </div>
                                                            </div>
