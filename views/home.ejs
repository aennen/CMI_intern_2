                                <div class="media-object stack-for-small">
                                    <div class="media-object-section">
                                        <div class="thumbnail">
                                            <img src="images/experience-now.png" class="medium">
                                        </div>
                                    </div>
                                    <div class="media-object-section">
                                        <div class="text">
                                            <h4><%= mediaObjectsWelcomeData[0].mediaTitle %></h4>
                                            <hr>
                                            <p><%= mediaObjectsWelcomeData[0].mediaText %></p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="sect_container">
                                <div class="media-object stack-for-small">
                                    <div class="media-object-section">
                                        <div class="text">
                                            <h4><%= mediaObjectsWelcomeData[1].mediaTitle %></h4>
                                            <hr>
                                            <p><%= mediaObjectsWelcomeData[1].mediaText %></p>
                                        </div>
                                    </div>
                                    <div class="media-object-section">
                                        <div class="thumbnail">
                                            <img src="images/amdocs-oss.png" class="medium">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="sect_container">
                                <div class="media-object stack-for-small">
                                    <div class="media-object-section">
                                        <div class="thumbnail">
                                            <img src="images/partner-frame.png" class="medium">
                                        </div>
                                    </div>
                                    <div class="media-object-section">
                                        <div class="text">
                                            <h4><%= mediaObjectsWelcomeData[2].mediaTitle %></h4>
                                            <hr>
                                            <p><%= mediaObjectsWelcomeData[2].mediaText %></p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="sect_container">
                                <div class="media-object stack-for-small">
                                    <div class="media-object-section">
                                        <div class="text">
                                            <h4><%= mediaObjectsWelcomeData[3].mediaTitle %></h4>
                                            <hr>
                                            <p><%= mediaObjectsWelcomeData[3].mediaText %></p>
                                            <p><br></p>
                                        </div>
                                    </div>
                                    <div class="media-object-section">
                                        <div class="thumbnail">
                                            <img src="images/logo-orange.png" class="medium">
                                        </div>
                                    </div>
